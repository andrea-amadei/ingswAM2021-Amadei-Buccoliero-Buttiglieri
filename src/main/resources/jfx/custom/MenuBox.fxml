<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root alignment="CENTER" style="-fx-border-color: black; -fx-border-width: 3; -fx-border-insets: 5;" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Menu">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Label>
      <GridPane fx:id="grid">
          <Button fx:id="backBtn" text="BACK" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0" GridPane.valignment="CENTER" />
          <Button fx:id="confirmButton" text="GO" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
          <Button fx:id="selectPlayCraftingBtn" text="CRAFTING" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
          <Button fx:id="selectPlayMarketBtn" text="MARKET" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
          <Button fx:id="selectPlayShopBtn" text="SHOP" GridPane.columnIndex="2" GridPane.rowIndex="1" />
          <Button fx:id="preliminaryPickBtn" text="PRELIMINARY PICK" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
